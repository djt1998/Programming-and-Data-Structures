public class SubtractionExpression extends Expression{
  public SubtractionExpression(String leftOp, String rightOp) throws InvalidQuestionException{
    super(leftOp,rightOp);
}
  public int evaluate(){
    return (leftOpInt - rightOpInt);
  }
}